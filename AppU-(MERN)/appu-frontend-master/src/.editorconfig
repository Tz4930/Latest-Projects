# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.css]
indent_style = space
indent_size = 2

[*.js]
indent_style = tab
indent_size = 4

[*.jsx]
indent_style = tab
indent_size = 4